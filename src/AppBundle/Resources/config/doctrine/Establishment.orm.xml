<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Establishment" table="establishment">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="256" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="region" type="string" column="region" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="address" type="string" column="address" length="512" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="postal" type="decimal" column="postal" precision="10" scale="0" nullable="false"/>
    <field name="city" type="string" column="city" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
